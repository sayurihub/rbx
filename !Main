local url = placeScripts[game.PlaceId]

if url then
    print("üåç Loading script for place:", game.PlaceId)
    loadstring(game:HttpGet(url))()
else
    warn("‚ùå This place is not in the list. PlaceId:", game.PlaceId)
end
